{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
    "baseUrl": "./",
    "rootDir": "./src",
    "outDir": "./dist",
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "paths": {
      "@netron": [
        "src/netron/index.js"
      ],
      "@netron/*": [
        "src/netron/*"
      ],
      "@nexus": [
        "src/nexus/index.js"
      ],
      "@nexus/*": [
        "src/nexus/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/*spec.ts"
  ]
}