{
  "name": "@holon/runtime",
  "version": "0.1.0",
  "description": "Flow-Machine runtime with distributed execution, service mesh, and production infrastructure",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js",
      "default": "./dist/runtime/index.js"
    },
    "./runtime/engine": {
      "types": "./dist/runtime/engine.d.ts",
      "import": "./dist/runtime/engine.js",
      "default": "./dist/runtime/engine.js"
    },
    "./runtime/executor": {
      "types": "./dist/runtime/executor.d.ts",
      "import": "./dist/runtime/executor.js",
      "default": "./dist/runtime/executor.js"
    },
    "./runtime/scheduler": {
      "types": "./dist/runtime/scheduler.d.ts",
      "import": "./dist/runtime/scheduler.js",
      "default": "./dist/runtime/scheduler.js"
    },
    "./mesh": {
      "types": "./dist/mesh/index.d.ts",
      "import": "./dist/mesh/index.js",
      "default": "./dist/mesh/index.js"
    },
    "./mesh/node": {
      "types": "./dist/mesh/node.d.ts",
      "import": "./dist/mesh/node.js",
      "default": "./dist/mesh/node.js"
    },
    "./mesh/router": {
      "types": "./dist/mesh/router.d.ts",
      "import": "./dist/mesh/router.js",
      "default": "./dist/mesh/router.js"
    },
    "./mesh/discovery": {
      "types": "./dist/mesh/discovery.d.ts",
      "import": "./dist/mesh/discovery.js",
      "default": "./dist/mesh/discovery.js"
    },
    "./distributed": {
      "types": "./dist/distributed/index.d.ts",
      "import": "./dist/distributed/index.js",
      "default": "./dist/distributed/index.js"
    },
    "./distributed/coordinator": {
      "types": "./dist/distributed/coordinator.d.ts",
      "import": "./dist/distributed/coordinator.js",
      "default": "./dist/distributed/coordinator.js"
    },
    "./distributed/worker": {
      "types": "./dist/distributed/worker.d.ts",
      "import": "./dist/distributed/worker.js",
      "default": "./dist/distributed/worker.js"
    },
    "./distributed/consensus": {
      "types": "./dist/distributed/consensus.d.ts",
      "import": "./dist/distributed/consensus.js",
      "default": "./dist/distributed/consensus.js"
    },
    "./viz": {
      "types": "./dist/viz/index.d.ts",
      "import": "./dist/viz/index.js",
      "default": "./dist/viz/index.js"
    },
    "./viz/graph": {
      "types": "./dist/viz/graph.d.ts",
      "import": "./dist/viz/graph.js",
      "default": "./dist/viz/graph.js"
    },
    "./viz/metrics": {
      "types": "./dist/viz/metrics.d.ts",
      "import": "./dist/viz/metrics.js",
      "default": "./dist/viz/metrics.js"
    },
    "./viz/debugger": {
      "types": "./dist/viz/debugger.d.ts",
      "import": "./dist/viz/debugger.js",
      "default": "./dist/viz/debugger.js"
    },
    "./integrations": {
      "types": "./dist/integrations/index.d.ts",
      "import": "./dist/integrations/index.js",
      "default": "./dist/integrations/index.js"
    },
    "./integrations/http": {
      "types": "./dist/integrations/http.d.ts",
      "import": "./dist/integrations/http.js",
      "default": "./dist/integrations/http.js"
    },
    "./integrations/grpc": {
      "types": "./dist/integrations/grpc.d.ts",
      "import": "./dist/integrations/grpc.js",
      "default": "./dist/integrations/grpc.js"
    },
    "./integrations/kafka": {
      "types": "./dist/integrations/kafka.d.ts",
      "import": "./dist/integrations/kafka.js",
      "default": "./dist/integrations/kafka.js"
    },
    "./integrations/redis": {
      "types": "./dist/integrations/redis.d.ts",
      "import": "./dist/integrations/redis.js",
      "default": "./dist/integrations/redis.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "holon": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "src",
    "!src/**/*.test.ts",
    "!src/**/*.spec.ts",
    "!src/**/*.bench.ts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "clean": "rm -rf dist coverage .turbo"
  },
  "keywords": [
    "flow",
    "runtime",
    "distributed",
    "service-mesh",
    "flow-machine",
    "execution-engine",
    "holon"
  ],
  "author": "Holon Team",
  "license": "MIT",
  "homepage": "https://github.com/holon-flow/holon#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holon-flow/holon.git",
    "directory": "packages/runtime"
  },
  "bugs": {
    "url": "https://github.com/holon-flow/holon/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@holon/flow": "workspace:*",
    "commander": "^14.0.1",
    "ioredis": "^5.8.1",
    "pino": "^10.0.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.8.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}