{
  "name": "orchestron",
  "version": "0.1.0",
  "description": "Comprehensive development orchestration system with multi-agent coordination and ML-powered analytics",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "orchestron": "./dist/cli.js",
    "orchestron-server": "./dist/orchestron/main.js"
  },
  "scripts": {
    "build": "npm run build:backend && npm run build:web",
    "build:backend": "tsc",
    "build:web": "vite build",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "link:global": "npm run build && npm link",
    "start": "npm run build && node dist/cli.js",
    "orchestron": "npm run build:backend && node dist/orchestron/main.js",
    "orchestron:dev": "tsx src/orchestron/main.ts",
    "mcp:server": "npm run build && node dist/mcp/server.js",
    "mcp:dev": "tsx src/mcp/server.ts",
    "server": "npm run build:backend && node dist/dashboard/server.js",
    "server:dev": "tsx src/dashboard/server.ts",
    "web": "vite",
    "web:build": "vite build",
    "web:preview": "vite preview"
  },
  "keywords": [
    "orchestration",
    "development-management",
    "multi-agent",
    "task-tracking",
    "project-management",
    "ml-analytics"
  ],
  "author": "Orchestron Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^8.2.0",
    "@hookform/resolvers": "^3.9.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@omnitron-dev/common": "workspace:*",
    "@omnitron-dev/eventemitter": "workspace:*",
    "@omnitron-dev/titan": "workspace:*",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.1.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.1.5",
    "@tanstack/react-virtual": "^3.13.10",
    "@types/ws": "^8.18.1",
    "@uiw/react-md-editor": "^4.0.7",
    "ansi-to-html": "^0.7.2",
    "better-sqlite3": "^12.4.1",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "date-fns": "4.1.0",
    "diff": "^8.0.2",
    "eventemitter3": "^5.0.1",
    "fastify": "^5.6.1",
    "framer-motion": "^12.0.0-alpha.1",
    "html2canvas": "^1.4.1",
    "idb": "8.0.3",
    "lucide-react": "^0.468.0",
    "pino": "^9.11.0",
    "pino-pretty": "^13.1.1",
    "posthog-js": "^1.258.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.0.3",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.14.1",
    "remark-gfm": "^4.0.0",
    "socket.io": "^4.6.1",
    "tailwind-merge": "^2.6.0",
    "uuid": "13.0.0",
    "ws": "^8.18.3",
    "zod": "4.1.11",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "@types/better-sqlite3": "7.6.13",
    "@types/cors": "^2.8.17",
    "@types/diff": "^8.0.0",
    "@types/node": "24.5.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "9.36.0",
    "happy-dom": "^18.0.1",
    "jest": "30.1.3",
    "tailwindcss": "^4.1.8",
    "ts-jest": "29.4.4",
    "tsx": "4.20.6",
    "typescript": "5.9.2",
    "vite": "^6.0.3",
    "vitest": "3.2.4"
  }
}
