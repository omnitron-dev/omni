{
  "name": "@kysera/audit",
  "version": "0.5.1",
  "description": "Audit logging plugin for Kysera ORM",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:multi-db": "vitest run test/multi-db.test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src test",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "dependencies": {
    "@kysera/core": "workspace:*",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@kysera/repository": "workspace:*",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "better-sqlite3": "^12.5.0",
    "kysely": "^0.28.8",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "@kysera/repository": "workspace:*",
    "kysely": ">=0.28.8"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kysera-dev/kysera.git",
    "directory": "packages/audit"
  },
  "keywords": [
    "kysely",
    "orm",
    "typescript",
    "audit",
    "logging",
    "tracking"
  ],
  "author": "Kysera Team",
  "license": "MIT",
  "sideEffects": false
}
